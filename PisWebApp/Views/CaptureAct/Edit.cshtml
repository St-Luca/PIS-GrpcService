@model PIS_GrpcService.PIS_GrpcService.GrpcCaptureAct

@{
    ViewData["Title"] = "Редактировать акт";
}

<h1>Акт</h1>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="ActDate"></label>
        <input asp-for="ActDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Animals"></label>
        <input asp-for="Animals" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Amount"></label>
        <input asp-for="Amount" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Performer"></label>
        <select asp-for="Locality" class="form-control" asp-items="ViewBag.orgId" />
    </div>
    <div class="form-group">
        <label asp-for="Locality"></label>
        <select asp-for="Locality" class="form-control" asp-items="ViewBag.locId" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-primary" />
    </div>
</form>
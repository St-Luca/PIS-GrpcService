@model PIS_GrpcService.PIS_GrpcService.GrpcOrganization

@{
    ViewData["Title"] = "Редактировать организацию";
}

<h1>Организация</h1>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="OrgName"></label>
        <input asp-for="OrgName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="INN"></label>
        <input asp-for="INN" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="KPP"></label>
        <input asp-for="KPP" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="OrgAddress"></label>
        <input asp-for="OrgAddress" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="OrgType"></label>
        <input asp-for="OrgType" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="OrgAttribute"></label>
        <input asp-for="OrgAttribute" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-primary" />
    </div>
</form>


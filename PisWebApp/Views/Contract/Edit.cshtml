@model PIS_GrpcService.PIS_GrpcService.GrpcContract

@{
    ViewData["Title"] = "Редактировать контракт";
}

<h1>Контракт</h1>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="ConclusionDate"></label>
        <input asp-for="ConclusionDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="EffectiveDate"></label>
        <input asp-for="EffectiveDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Amount"></label>
        <input asp-for="Amount" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Performer"></label>
        <input asp-for="Performer" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="LocalityCosts"></label>
        <input asp-for="LocalityCosts" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-primary" />
    </div>
</form>